{
  "guilds": [
    <% if !@guilds.blank? %>
      <% @guilds.each do |guild| %>
        {
          "name": "<%= guild.name %>",
          "realm": "<%= guild.realm %>",
          "rps": "<%= guild.format_realm_points(guild.get_rps_from_duration(@duration)) %>",
          "character_count": "<%= guild.players.count %>"
        }<% if guild != @guilds.last %>,<% end %>
      <% end %>
    <% end %>
  ]
}