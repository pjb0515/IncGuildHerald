{
  "players": [
    <% if !@players.blank? %>
      <% @players.each do |player| %>
        "name": "<%= player.name %>",
        "realm": "<%= player.realm %>",
        "guild": "<%= player.guild.name %>",
        "rps": "<%= player.get_rps_from_duration(@duration) %>",
        "realm_level": "<%= player.realm_level %>",
        "daoc_class": "<%= player.daoc_class.capitalize %>"
      <% end %>
    <% end %>
  ]
}